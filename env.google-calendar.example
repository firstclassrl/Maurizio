# Google Calendar Sync Configuration
# Copy this file to .env.local and fill in your values

# Google OAuth2 Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=https://yourdomain.com/api/google-oauth/callback

# Webhook Configuration
WEBHOOK_BASE_URL=https://lexagenda.it
WEBHOOK_SECRET=your_webhook_secret_here

# Supabase Configuration (already configured in your main .env)
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Optional: Google Calendar API Settings
GOOGLE_CALENDAR_SYNC_ENABLED=true
GOOGLE_CALENDAR_MAX_EVENTS_PER_SYNC=1000
GOOGLE_CALENDAR_SYNC_INTERVAL_HOURS=24

# Optional: Rate Limiting
GOOGLE_API_RATE_LIMIT_PER_MINUTE=100
GOOGLE_API_RATE_LIMIT_PER_DAY=1000000
